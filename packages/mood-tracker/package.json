{
  "name": "mood-tracker",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun run index.ts",
    "build": "bun build index.ts --outdir build --target node",
    "reset-db": "rm -f mood-tracker.db && echo 'Database reset successfully'",
    "mcp-inspector": "bunx @modelcontextprotocol/inspector bun run index.ts -e DB_FILE_NAME=$DB_FILE_NAME",
    "logs": "cat mood-tracker.log || echo 'No logs found'",
    "watch-logs": "tail -f mood-tracker.log || echo 'No logs found'"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.3",
    "bun-types": "latest",
    "drizzle-orm": "latest",
    "zod": "latest"
  }
}